<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="resources\images\arrow_next.png" />
    <None Remove="resources\images\arrow_prev.png" />
    <None Remove="resources\images\hedgehog_blue.png" />
    <None Remove="resources\images\hedgehog_green.png" />
    <None Remove="resources\images\hedgehog_red.png" />
    <None Remove="resources\images\icon.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="resources\images\arrow_next.png" />
    <Resource Include="resources\images\arrow_prev.png" />
    <Resource Include="resources\images\hedgehog_blue.png" />
    <Resource Include="resources\images\hedgehog_green.png" />
    <Resource Include="resources\images\hedgehog_red.png" />
    <Resource Include="resources\images\icon.ico" />
  </ItemGroup>

</Project>
